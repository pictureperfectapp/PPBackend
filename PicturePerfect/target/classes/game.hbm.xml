<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name = "com.revature.beans.Game" table = "Game">
		<meta attribute = "class-description">
			This is a table of Games
		</meta>
		<id name = "id" type = "int" column = "g_id">
			<generator class = "native">
				<param name = "sequence">
					MY_SEQ
				</param>
			</generator>
		</id>
		<property name = "word" type = "string" not-null = "true"/>
		<property name = "picture" type = "blob"/>
		<property name = "guess" type = "String"/>
		<property name = "userturnid" type = "int"/>
	</class>
</hibernate-mapping>