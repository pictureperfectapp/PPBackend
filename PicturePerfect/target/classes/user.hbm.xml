<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name = "com.revature.beans.User" table = "User">
		<meta attribute = "class-description">
			This is a table of Users
		</meta>
		<id name = "id" type = "int" column = "u_id">
			<generator class = "native">
				<param name = "sequence">
					MY_SEQ
				</param>
			</generator>
		</id>
		<property name = "username" type = "string" not-null = "true"/>
		<property name = "password" type = "string" not-null = "true"/>
		<property name = "email" type = "string" not-null = "true"/>
		<property name = "admin" type = "string" not-null = "true"/>
	</class>
</hibernate-mapping>