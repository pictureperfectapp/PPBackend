<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name = "com.revature.beans.Report" table = "Report">
		<meta attribute = "class-description">
			This is a table of Reports submitted by users
		</meta>
		<id name = "id" type = "int" column = "r_id">
			<generator class = "native">
				<param name = "sequence">
					MY_SEQ
				</param>
			</generator>
		</id>
		<property name = "u_id" type = "int" not-null = "true"/>
		<property name = "g_id" type = "int" not-null = "true"/>
		<property name = "staus" type = "string" not-null = "true"/>
		<property name = "description" type = "string"/>
	</class>
</hibernate-mapping>