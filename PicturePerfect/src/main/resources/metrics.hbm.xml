<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name = "com.revature.beans.Metrics" table = "Metrics">
		<meta attribute = "class-description">
			This is a table of Player Metrics
		</meta>
		<id name = "id" type = "int" column = "m_id">
			<generator class = "native">
				<param name = "sequence">
					MY_SEQ
				</param>
			</generator>
		</id>
		<property name = "u_id" type = "int" not-null = "true"/>
		<property name = "points" type = "int"/>
		<property name = "gamesPlayed" type = "int"/>
		<property name = "wins" type = "int"/>
	</class>
</hibernate-mapping>